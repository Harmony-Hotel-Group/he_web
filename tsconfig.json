{
  "extends": "astro/tsconfigs/base",
  "include": [
    "src/**/*",
    "src/env.d.ts",
    ".astro/types.d.ts",
    "astro.config.mjs",
    "tailwind.config.cjs"
  ],
  "exclude": [
    "./node_modules",
    ".vscode",
    "dist"
  ],
  "compilerOptions": {
    "baseUrl":".",
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "paths": {
      "~/*": ["./src/*"],
    }
  }
}
